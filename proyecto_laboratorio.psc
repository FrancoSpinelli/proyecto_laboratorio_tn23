		FinSegun
		
		si jugadorMano = 1 Entonces
			jugadorMano <-2;
		SiNo
			jugadorMano <-1;
		FinSi	
	FinPara
	
	Escribir  "Puntaje de la partida jugador 1: ", puntajeJ1 , " jugador 2:" , puntajeJ2;
	puntajeJ1Gral <- puntajeJ1Gral + puntajeJ1;
	puntajeJ2Gral <- puntajeJ1Gra2 + puntajeJ2;
	
